{
  "firestore": {
    "database": "(default)",
    "location": "nam5",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/login",
        "destination": "/login.html",
        "type": 302
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/login.html"
      }
    ]
  }
}
